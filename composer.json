{
  "name": "tourze/doctrine-use-index-walker",
  "description": "Auto add USE INDEX in doctrine query",
  "type": "library",
  "license": "MIT",
  "require": {
    "doctrine/dbal": "^4.0",
    "doctrine/orm": "^3.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineUseIndexWalker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineUseIndexWalker\\Tests\\": "tests/"
    }
  }
}